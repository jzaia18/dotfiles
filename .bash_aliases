# Git related commands
alias aa='git add .'
alias pull='git pull && ls'
alias push='git push'
alias pushu='git push -u origin $(git branch --show-current)'
alias pullsh='pull && push'
alias commit='git commit'
alias clone='git clone'
alias chout='git checkout'
